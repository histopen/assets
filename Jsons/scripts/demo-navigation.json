{
  "name": "Navigation Demo",
  "description": "Demonstrates pan, zoom, and jump commands",
  "commands": [

    {"command": "jumpToDate", "params": {"year": 2024}},                               {"command": "delay", "params": {"ms": 500}},
    {"command": "zoom", "params": {"direction": "in", "centerX": 500}},                {"command": "delay", "params": {"ms": 500}},
    {"command": "zoom", "params": {"direction": "in", "centerX": 500}},                {"command": "delay", "params": {"ms": 500}},
    {"command": "zoom", "params": {"direction": "in", "centerX": 500}},                {"command": "delay", "params": {"ms": 500}},
    {"command": "pan", "params": {"deltaX": 300, "animated": true}},                   {"command": "delay", "params": {"ms": 500}},
    {"command": "pan", "params": {"deltaX": -300, "animated": true}},                  {"command": "delay", "params": {"ms": 500}},
    {"command": "jumpToZoomScale", "params": {"zoomScaleId": 27}},                     {"command": "delay", "params": {"ms": 500}},
    {"command": "jumpToEra", "params": {"start": "1789/7/14", "end": "1799/11/9"}},    {"command": "delay", "params": {"ms": 500}}

  ]
}
